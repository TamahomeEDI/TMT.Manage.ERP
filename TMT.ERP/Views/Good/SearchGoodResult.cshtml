@model TMT.ERP.DataAccess.Model.Good

@{
    Layout = null;
    ViewBag.Title = "Search";
}
<table id="BankAccount" class="table">
    <thead>
        <tr>
            <th>
                @Resources.Resource.Good_Create_Code
            </th>
            <th>
                @Resources.Resource.Good_Create_Name
            </th>
            <th>
                @Resources.Resource.Good_Create_Attachment
            </th>
            @*<th>
                @Html.DisplayNameFor(model => model.ContactID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UOMID)
            </th>*@
            <th>
                @Resources.Resource.Good_Create_InputPrice
            </th>
            <th>
                @Resources.Resource.Good_Create_OutputPrice
            </th>
            <th>
                @Resources.Resource.Good_Create_DomesticPrice
            </th>
            <th>
                @Resources.Resource.Good_Create_StartDate
            </th>
            @*<th>
                @Html.DisplayNameFor(model => model.PeriodDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>*@
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Code)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Attachment)
                </td>
                @*<td>
                    @Html.DisplayFor(modelItem => item.ContactID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Type)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UOMID)
                </td>*@
                <td>
                    @Html.DisplayFor(modelItem => item.InputPrice)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OutputPrice)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DomesticPrice)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.StartedDate)
                </td>
                @*<td>
                    @Html.DisplayFor(modelItem => item.PeriodDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Description)
                </td>*@
                <td>
                    <a id="GoodEditID">@Html.HiddenFor(modelItem => item.ID) @Resources.Resource.Account_Index_Edit</a>|
                        <a id="GoodDetailID">@Html.HiddenFor(modelItem => item.ID) @Resources.Resource.Account_Index_Detail </a>|
                        <a id="GoodDeleteID">@Html.HiddenFor(modelItem => item.ID) @Resources.Resource.Account_Index_Delete </a>
                </td>
            </tr>
        }
    </tbody>
</table>
